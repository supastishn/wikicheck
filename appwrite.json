{
    "projectId": "wikicheck",
    "functions": [
        {
            "$id": "llm-search",
            "name": "llm-search",
            "runtime": "node-22",
            "specification": "s-0.5vcpu-512mb",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "users.read"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/llm-search"
        }
    ],
    "databases": [
        {
            "$id": "factchecks",
            "name": "factchecks",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "checks",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")"
            ],
            "databaseId": "factchecks",
            "name": "checks",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [],
            "indexes": []
        }
    ]
}